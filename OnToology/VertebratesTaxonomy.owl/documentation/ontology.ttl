@prefix : <http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy> .

<http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy> rdf:type owl:Ontology ;
                                                                 <http://purl.org/dc/terms/description> "An ontology about vertebrates and some of their features" ;
                                                                 owl:author "Sara Shahmohamadi" ;
                                                                 owl:versionInfo 1.0 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#author
owl:author rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#description
owl:description rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#prefLabel
owl:prefLabel rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#eats
:eats rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      <http://purl.org/dc/terms/description> "A relation between animals, which animal preys on and eats which."@en ;
      rdfs:label "isst"@de ,
                 "mange"@fr ,
                 "می‌خورد"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasBodyTemperature
:hasBodyTemperature rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Animal ;
                    rdfs:range :BodyTemperature ;
                    <http://purl.org/dc/terms/description> "A relation specifying what the body temperature is." ;
                    rdfs:label "a la température corporelle"@fr ,
                               "hat Körpertemperatur"@de ,
                               "دمای بدن برابر با"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasConservationStatus
:hasConservationStatus rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain :Animal ;
                       rdfs:range :ConservationStatus ;
                       <http://purl.org/dc/terms/description> "A property specifying what the conservation status of an animal is." ;
                       rdfs:label "a le statue de conservation"@fr ,
                                  "hat Erhaltungszustand"@de ,
                                  "دارای وضعیت بقای"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasDiet
:hasDiet rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Animal ;
         rdfs:range :Diet ;
         <http://purl.org/dc/terms/description> "hat Diät"@de ,
                                                "A property specifying food habits of animals." ;
         rdfs:label "a un régime"@fr ,
                    "پیرو رژیم غذایی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasRespiratorySystem
:hasRespiratorySystem rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain :Animal ;
                      rdfs:range :RespiratorySystem ;
                      rdfs:label "a le système respiratoire"@fr ,
                                 "hat Atmungssystem"@de ,
                                 "دارای دستگاه تنفسی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasSkinCover
:hasSkinCover rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Animal ;
              rdfs:range :SkinCovering ;
              <http://purl.org/dc/terms/description> "A property specifying animals' body coverings." ;
              rdfs:label "a le revêtement corporel"@fr ,
                         "hatKörperbedeckung"@de ,
                         "دارای پوشش بدنی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasTameness
:hasTameness rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Animal ;
             rdfs:range :Tameness ;
             <http://purl.org/dc/terms/description> "A relation specifying tameness of animals." ;
             rdfs:label "a la domicilité"@fr ,
                        "hatZahmheit"@de ,
                        "واجد خصوصیت رامی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasTaxonomicRrank
:hasTaxonomicRrank rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :Animal ;
                   rdfs:range :TaxonomicRank ;
                   <http://purl.org/dc/terms/description> "Representing taxonomic ranks of a given creature; i.e. is a class, a phylum, a kingdom etc." ;
                   rdfs:label "a le rang taxonomique"@fr ,
                              "hatTaxonomischenRang"@de ,
                              "دارای رتبه آرایه‌شناختی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#livesInClimate
:livesInClimate rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Animal ;
                rdfs:range :Climate ;
                <http://purl.org/dc/terms/description> "A property specifying what kind of climate(s) animals live in." ;
                rdfs:label "lebt im Klima"@de ,
                           "vit dans le climat"@fr ,
                           "ساکن در اقلیم"@fa .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasAquaticHabitat
:hasAquaticHabitat rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :hasHabitat ;
                   rdfs:domain :Animal ;
                   rdfs:range xsd:string ;
                   <http://purl.org/dc/terms/description> "A data property specifying aquatic habitats of animals." ;
                   rdfs:label "aquatischerLebensraumIst"@de ,
                              "habitatAquatiqueEst"@fr ,
                              "دارای سکونتگاه آبی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasAverageBodyLength
:hasAverageBodyLength rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdf:type owl:FunctionalProperty ;
                      rdfs:domain :Animal ;
                      rdfs:range xsd:float ;
                      <http://purl.org/dc/terms/description> "A data property specifying the average length of animals." ;
                      rdfs:label "aUneLongeurMoyenne"@fr ,
                                 "hatdurchschnittlicheLänge"@de ,
                                 "دارای میانگین طول بدن"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasAverageHeight
:hasAverageHeight rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain :Animal ;
                  rdfs:range xsd:float ;
                  <http://purl.org/dc/terms/description> "A data property specifying average height of animals." ;
                  rdfs:label "aUneHauteurMoyenne"@fr ,
                             "hatdurchschnittlicheHöhe"@de ,
                             "دارای میانگین قد"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasAverageLifespan
:hasAverageLifespan rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain :Animal ;
                    rdfs:range xsd:float ;
                    <http://purl.org/dc/terms/description> "A data property specifying average lifespan of animals." ;
                    rdfs:label "aUneDuréeDeVieMoyenne"@fr ,
                               "hatEineDurchschnittlicheLebensdauer"@de ,
                               "دارای میانگین طول عمر"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasClassName
:hasClassName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :hasTaxonName ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Animal ;
              rdfs:range xsd:string ;
              <http://purl.org/dc/terms/description> "A data property specifying the class names of animals." ;
              rdfs:label "KlassenameIst"@de ,
                         "nomDeClasseEst"@fr ,
                         "دارای نام رده"@fa .

[ rdf:type owl:AllDisjointProperties ;
   owl:members ( :hasClassName
               )
 ] .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasColor
:hasColor rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Animal ;
          rdfs:range xsd:string ;
          <http://purl.org/dc/terms/description> "A data property specifying color(s) of animals." ;
          rdfs:label "estDeCouleur"@fr ,
                     "vonFarbe"@de ,
                     "دارای رنگ‌[های]"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasCommanName
:hasCommanName rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Animal ;
               rdfs:range xsd:string ;
               <http://purl.org/dc/terms/description> "A data property specifying the common names of animals." ;
               rdfs:label "gebräuchlicherNameIst"@de ,
                          "nomCommunEst"@fr ,
                          "دارای اسم عامیانه"@fa .

[ rdf:type owl:AllDisjointProperties ;
   owl:members ( :hasCommanName
               )
 ] .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasEnglishName
:hasEnglishName rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :hasCommanName ;
                owl:propertyDisjointWith :hasPersianName ;
                <http://purl.org/dc/terms/description> "A data property specifying common English names for animals." ;
                rdfs:label "englischerGebräuchlicherNameIst"@de ,
                           "nomCommunAnglaisEst"@fr ,
                           "دارای اسم عامیانه انگلیسی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasFamilyName
:hasFamilyName rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :hasTaxonName ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Animal ;
               rdfs:range xsd:string ;
               <http://purl.org/dc/terms/description> "A data property specifying the Family names of animals." ;
               rdfs:label "FamilienameIst"@de ,
                          "nomDeFamilleEst"@fr ,
                          "دارای نام خانواده"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasGenusName
:hasGenusName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :hasTaxonName ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Animal ;
              rdfs:range xsd:string ;
              <http://purl.org/dc/terms/description> "A data property specifying the genus names of animals." ;
              rdfs:label "gattungnameIst"@de ,
                         "nomDeGenreEst"@fr ,
                         "دارای نام سرده"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasHabitat
:hasHabitat rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Animal ;
            rdfs:range xsd:string ;
            <http://purl.org/dc/terms/description> "A data property specifying the habitat of animals." ;
            rdfs:label "habitatEst"@fr ,
                       "lebensraumIst"@de ,
                       "دارای سکونتگاه"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasKingdomName
:hasKingdomName rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :hasTaxonName ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :Animal ;
                rdfs:range xsd:string ;
                <http://purl.org/dc/terms/description> "A data property specifying the kingdom names of animals." ;
                rdfs:label "ReichnameIst"@de ,
                           "nomDeRègneEst"@fr ,
                           "دارای نام فرمانرو"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasOrderName
:hasOrderName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :hasTaxonName ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Animal ;
              rdfs:range xsd:string ;
              <http://purl.org/dc/terms/description> "A data property specifying the order names of animals." ;
              rdfs:label "nomDeOrdreEst"@fr ,
                         "ordnungnameIst"@de ,
                         "دارای نام راسته"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasPersianName
:hasPersianName rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :hasCommanName ;
                <http://purl.org/dc/terms/description> "A data property specifying common Farsi name for animals." ;
                rdfs:label "gebräuchlicherFarsiName"@de ,
                           "nomCommunPersanEst"@fr ,
                           "دارای اسم عامیانه فارسی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasPhylumName
:hasPhylumName rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :hasTaxonName ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Animal ;
               rdfs:range xsd:string ;
               <http://purl.org/dc/terms/description> "A data property specifying the phylum names of animals." ;
               rdfs:label "StammnameIst"@de ,
                          "nomDeEmbranchementEst"@fr ,
                          "دارای نام شاخه"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasSpeciesName
:hasSpeciesName rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :hasTaxonName ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :Animal ;
                rdfs:range xsd:string ;
                <http://purl.org/dc/terms/description> "A data property specifying the species names of animals." ;
                rdfs:label "artnameIst"@de ,
                           "nomDeEspèceEst"@fr ,
                           "دارای نام گونه"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasTaxonName
:hasTaxonName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Animal ;
              rdfs:range xsd:string ;
              <http://purl.org/dc/terms/description> "A data property specifying the taxonomic names of animals." ;
              rdfs:label "nomTaxonomiqueEst"@fr ,
                         "taxonomischernameIst"@de ,
                         "دارای نام آرایه‌شناختی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasTerrestrialHabitat
:hasTerrestrialHabitat rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :hasHabitat ;
                       rdfs:domain :Animal ;
                       rdfs:range xsd:string ;
                       <http://purl.org/dc/terms/description> "A data property specifying terrestrial habitats of animals." ;
                       rdfs:label "habitatTerrestreEst"@fr ,
                                  "terrestrischerLebensraumIst"@de ,
                                  "دارای سکونتگاه زمینی"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasTopSpeed
:hasTopSpeed rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Animal ;
             rdfs:range xsd:float ;
             <http://purl.org/dc/terms/description> "A data property specifying the top speed of animals." ;
             rdfs:label "aLaVitesseMaximale"@fr ,
                        "hatHöchstgeschwindigkeit"@de ,
                        "دارای حداکثر سرعت"@fa .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Amphibian
:Amphibian rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Vertebrate
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasBodyTemperature ;
                                                        owl:hasValue :Changing
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasRespiratorySystem ;
                                                        owl:hasValue :Other
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           <http://purl.org/dc/terms/description> "Representing different kinds of amphibians" ;
           rdfs:label "Amphibie"@de ,
                      "amphibie"@fr .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Animal
:Animal rdf:type owl:Class ;
        rdfs:subClassOf :DomainThing ;
        <http://purl.org/dc/terms/description> "Representing all living things categorized under the kingdom Animalia." ;
        rdfs:label "Animal"@en ,
                   "Animal"@fr ,
                   "Tier"@de ,
                   "حیوان"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Bird
:Bird rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :Vertebrate
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasBodyTemperature ;
                                                   owl:hasValue :Constant
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasSkinCover ;
                                                   owl:hasValue :Feather
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      <http://purl.org/dc/terms/description> "Representing different kinds of birds" ;
      rdfs:label "Vogel"@de ,
                 "oiseau"@fr .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#BodyTemperature
:BodyTemperature rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :ValueThing
                                                            [ rdf:type owl:Class ;
                                                              owl:oneOf ( :Changing
                                                                          :Constant
                                                                        )
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :ValueThing ;
                 <http://purl.org/dc/terms/description> "This is a class representing body temperature of animals. It has two individuals: changing and constant. Constant body temperature includes to warm blooded creatures and changing body temperature indludes cold blooded ones." ;
                 rdfs:label "Körpertemperatur"@de ,
                            "température corporelle"@fr ,
                            "دمای بدن"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Climate
:Climate rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :ValueThing
                                                    [ rdf:type owl:Class ;
                                                      owl:oneOf ( :Arctic
                                                                  :Cold
                                                                  :Moderate
                                                                  :Tropical
                                                                )
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         <http://purl.org/dc/terms/description> "Representing different types of climates animals could live in. Class Climate includes four instances: Arctic, Tropical, Moderate, Cold." ;
         rdfs:label "Klima"@de ,
                    "climat"@fr ,
                    "اقلیم"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#ColdBlooded
:ColdBlooded rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Animal
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :hasBodyTemperature ;
                                                          owl:hasValue :Changing
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Animal ;
             owl:disjointWith :WarmBlooded ;
             rdfs:label "ColdBlooded"@en ,
                        "ectothermique"@fr ,
                        "ektothermisch"@de ,
                        "خونسرد"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#ConservationStatus
:ConservationStatus rdf:type owl:Class ;
                    rdfs:subClassOf :ValueThing ;
                    rdfs:label "وضعیت بقا"@fa ;
                    rdfs:seeAlso "Erhaltungszustand"@de ,
                                 "le statue de conservation"@fr ,
                                 "Representing the conservation status of animals." .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Diet
:Diet rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :ValueThing
                                                 [ rdf:type owl:Class ;
                                                   owl:oneOf ( :Carnivore
                                                               :Herbivore
                                                               :Omnivore
                                                             )
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :ValueThing ;
      rdfs:label "رژیم غذایی"@fa ;
      rdfs:seeAlso "Diät"@de ,
                   "régime"@fr ,
                   "Representing animal diets." .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#DomainThing
:DomainThing rdf:type owl:Class ;
             owl:disjointWith :ValueThing ;
             <http://purl.org/dc/terms/description> "Representing our main concept of interest and class hierarchy: Animals, or to be more specific vertebrates." ;
             rdfs:label "DomainThing" ;
             owl:prefLabel "Domain Thing" .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Extinct
:Extinct rdf:type owl:Class ;
         rdfs:subClassOf :ConservationStatus ;
         <http://purl.org/dc/terms/description> "Representing the conservation status(es) considered as disappeared. It has two instances: One representing extinct as in extinct in the wild and the other, as in completely extinct." ;
         rdfs:label "ausgestorben"@de ,
                    "disparu"@fr ,
                    "منقرض‌شده"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Fish
:Fish rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :Vertebrate
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasBodyTemperature ;
                                                   owl:hasValue :Changing
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasRespiratorySystem ;
                                                   owl:hasValue :Gills
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :Vertebrate ;
      <http://purl.org/dc/terms/description> "Representating  different kinds of fishes" ;
      rdfs:label "Fisch"@de ,
                 "poisson"@fr .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#LowerRisk
:LowerRisk rdf:type owl:Class ;
           rdfs:subClassOf :ConservationStatus ;
           <http://purl.org/dc/terms/description> "Representing the coservation status(es) considered as safe, i.e. plenty of animals exist if the conservation status of an animal is of type lower risk. It includes only one instance: LeastConcern." ;
           rdfs:label "abondant"@fr ,
                      "reichlich"@de ,
                      "فراوان"@fa .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Mammal
:Mammal rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Vertebrate
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasBodyTemperature ;
                                                     owl:hasValue :Constant
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasSkinCover ;
                                                     owl:hasValue :Hair
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        <http://purl.org/dc/terms/description> "Representing different kinds of mammals"@fr ;
        rdfs:label "Säugetier"@de ,
                   "mammifère"@fr .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Marsupial
:Marsupial rdf:type owl:Class ;
           rdfs:subClassOf :Mammal ;
           <http://purl.org/dc/terms/description> "Representing one of the three main groups of living mammals; whose distinctive characteristic is that most of the young are carried in a pouch. Well-known marsupials include kangaroos, wallabies, koalas, possums, opossums, wombats, and Tasmanian devils." ;
           rdfs:label "Beuteltier"@de ,
                      "marsupial"@fr .


###  http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Monotreme
:Monotreme rdf:type owl:Class ;
           rdfs:subClassOf :Mammal ;
