        <rdfs:domain rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Animal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <owl:description>A data property specifying the common names of animals.</owl:description>
    </owl:DatatypeProperty>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointProperties"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasCommanName"/>
        </owl:members>
    </rdf:Description>
    


    <!-- http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasEnglishName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasEnglishName">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasCommanName"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasPersianName"/>
        <owl:description>A data property specifying common English names for animals.</owl:description>
    </owl:DatatypeProperty>
    


