    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Amphibian -->

    <owl:Class rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Amphibian">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Vertebrate"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasBodyTemperature"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Changing"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasRespiratorySystem"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Other"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:description>Representing different kinds of amphibians</owl:description>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Animal -->

    <owl:Class rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Animal">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#DomainThing"/>
        <rdfs:label xml:lang="en">Animal</rdfs:label>
        <owl:description>Representing all living things categorized under the kingdom Animalia.</owl:description>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Bird -->

    <owl:Class rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Bird">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Vertebrate"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasBodyTemperature"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Constant"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#hasSkinCover"/>
                        <owl:hasValue rdf:resource="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Feather"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:description>Representing different kinds of birds</owl:description>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#BodyTemperature -->

    <owl:Class rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#BodyTemperature">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#ValueThing"/>
                    <owl:Class>
                        <owl:oneOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Changing"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/sara/ontologies/VertebrateTaxonomy#Constant"/>
                        </owl:oneOf>
